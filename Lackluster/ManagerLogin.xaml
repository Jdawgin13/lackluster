<Window x:Class="Lackluster.ManagerLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lackluster"
        mc:Ignorable="d"
        Title="ManagerLogin" Height="160" Width="300" ResizeMode="NoResize">
    <Grid>
        <Label Content="Manager Must Login To Confirm New User." HorizontalAlignment="Left" Height="28" Margin="22,10,0,0" VerticalAlignment="Top" Width="238"/>
        <Label Content="Username:" HorizontalAlignment="Left" Height="28" Margin="22,38,0,0" VerticalAlignment="Top" Width="68"/>
        <Label Content="Password:" HorizontalAlignment="Left" Height="28" Margin="22,66,0,0" VerticalAlignment="Top" Width="69"/>
        <Button Content="Login" HorizontalAlignment="Left" Height="20" Margin="105,94,0,0" VerticalAlignment="Top" Width="85" Click="Button_Click"/>
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="23" Margin="95,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="153"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="95,66,0,0" VerticalAlignment="Top" Width="153"/>

    </Grid>
</Window>
